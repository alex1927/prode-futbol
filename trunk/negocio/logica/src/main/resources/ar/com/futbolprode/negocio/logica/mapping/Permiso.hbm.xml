<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="ar.com.futbolprode.negocio.entidades.modelo.usuario">
  <class name="Permiso" lazy="false" table="permiso">
  	<id access="field" column="permiso_id" name="id" type="integer">
  		<generator class="increment"/>
  	</id>
  	<property name="nombre" access="field" column="nombre" type="string"/>
  	<property name="fechaCreacion" access="field" type="date" />
  </class>
</hibernate-mapping>