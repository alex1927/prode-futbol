<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="ar.com.futbolprode.negocio.entidades.modelo.club">
  <class name="EstadoClub" table="estado_club" lazy="false">
  	<id access="field" column="estado_club_id" type="integer" name="id">
  		<generator class="increment"/>
  	</id>
  	<property name="descripcion" access="field" column="descripcion" type="string"/>
  	<property name="fechaCreacion" access="field" column="fecha_creacion" type="date" />
  </class>
</hibernate-mapping>