<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="ar.com.futbolprode.negocio.entidades.modelo.grupo">
  <class name="Grupo" table="grupo" lazy="false">
  <id access="field" column="grupo_id" type="integer" name="id">
  	<generator class="increment"/>
  </id>
  <property name="descripcion" access="field" column="descripcion" type="string"/>
  <property name="nombre" access="field" column="nombre" type="string"/>  
  </class>
</hibernate-mapping>