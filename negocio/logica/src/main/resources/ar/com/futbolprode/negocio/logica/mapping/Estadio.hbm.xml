<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="ar.com.futbolprode.negocio.entidades.modelo.liga">
  <class name="Estadio" table="estadio" lazy="false">
  	<id access="field" column="estadio_id" type="integer" name="id">
  		<generator class="increment" />
  	</id>
  	<property name="nombre" access="field" column="nombre" type="string" />
  	<property name="descripcion" access="field" column="descripcion" type="string" />
  	<property name="fechaCreacion" access="field" column="fecha_creacion" type="date"/>
  </class>
</hibernate-mapping>