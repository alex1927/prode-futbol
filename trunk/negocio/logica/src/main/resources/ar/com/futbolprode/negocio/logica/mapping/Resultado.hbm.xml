<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="ar.com.futbolprode.negocio.entidades.modelo.torneo">
  <class name="Resultado" table="resultado" lazy="false">
  	<id access="field" column="resultado_id" name="id" type="integer">
  		<generator class="increment"/>
  	</id>
  	<property name="valor" type="character" column="valor" access="field"/>
  	<property name="observaciones" type="string" column="observaciones" access="field"/>
  	<property name="fechaCreacion" type="date" access="field" column="fecha_creacion"/>
  </class>
</hibernate-mapping>